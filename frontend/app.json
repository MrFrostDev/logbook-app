{
  "expo": {
    "name": "frontend",
    "slug": "frontend",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#2a292b"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": "AIzaSyDWHmHeyozyMF5C6OssNa3M0FcMXkv1m1Y" //TODO: add your Google Maps API key here! 
      }
    },
    "android": {
      "permissions":["android.permission.ACCESS_BACKGROUND_LOCATION","android.permission.ACCESS_FINE_LOCATION","android.permission.ACCESS_COARSE_LOCATION" ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDWHmHeyozyMF5C6OssNa3M0FcMXkv1m1Y" //TODO: add your Google Maps API key here!
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#2a292b"
      },
      "package": "de.anwentw.logbook"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
